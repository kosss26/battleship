{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/((?!api/).*)",
      "dest": "/index.html"
    }
  ],
  "regions": ["fra1"],
  "framework": null,
  "outputDirectory": "dist"
}